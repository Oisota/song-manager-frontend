{
	"compilerOptions": {
		"strict": true,
		"module": "es2015",
		"moduleResolution": "node",
		"sourceMap": true,
		"target": "es2017",
		"esModuleInterop": true,
		"noImplicitAny": false,
		"lib": [
			"dom",
			"es6",
			"es2015.core"
		],
		"baseUrl": "./",
		"paths": {
			"jquery": ["jquery/dist/jquery.slim.js"],
			"App/*": ["src/*"],
			"Components/*": ["src/components/*"],
			"assets/*": ["assets/*"]
		}
	}
}
